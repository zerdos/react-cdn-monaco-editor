{
    "name": "react-cdn-monaco-editor",
    "version": "1.8.3",
    "description": "Monaco Editor loaded from CDN",
    "main": "index.js",
    "sideEffects": false,
    "scripts": {
        "build": "tsc && sed -i 's/VERSION/'$npm_package_version'/' ./lib/index.js && cp -f lib/* ./",
        "clean": "rimraf lib index.js",
        "format": "deno fmt srv",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "monaco",
        "editor",
        "react",
        "html",
        "vscode"
    ],
    "authors": [
        "Zoltan Erdos <zolika84@gmail.com>"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/zerdos/react-cdn-monaco-editor/issues"
    },
    "homepage": "https://github.com/zerdos/react-cdn-monaco-editor",
    "repository": "https://github.com/zerdos/react-cdn-monaco-editor",
    "devDependencies": {
        "@types/react": "^16.9.52",
        "@types/webpack-env": "^1.15.3",
        "monaco-editor": "^0.21.2",
        "typescript": "^4.0.3"
    }
}
