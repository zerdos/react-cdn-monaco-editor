{
    "name": "react-cdn-monaco-editor",
    "version": "1.8.9",
    "description": "Monaco Editor for React",
    "main": "index.js",
    "sideEffects": false,
    "scripts": {
        "build": "yarn format && tsc && sed -i 's/VERSION/'$npm_package_version'/' ./lib/index.js && cp -f lib/* ./",
        "clean": "rimraf lib index.js",
        "format": "deno fmt src && deno fmt example/src",
        "prepublishOnly": "npm run build && cd example && yarn && cp -r ../lib node_modules/react-cdn-monaco-editor/ && yarn build && cd .. && git add .&& git commit -m prepublish-$npm_package_version && git push"
    },
    "keywords": [
        "monaco",
        "editor",
        "react",
        "vscode"
    ],
    "authors": [
        "Daniele Esposti <daniele.espsoti@gmail.com>",
        "Dominik Moritz <domoritz@cs.washington.edu>",
        "Leon Shi <superRaytin@gmail.com>",
        "Zoltan Erdos <zolika84@gmail.com>"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/zerdos/react-cdn-monaco-editor/issues"
    },
    "homepage": "https://github.com/zerdos/react-cdn-monaco-editor",
    "repository": "https://github.com/zerdos/react-cdn-monaco-editor",
    "devDependencies": {
        "@types/react": "^16.9.52",
        "@types/webpack-env": "^1.15.3",
        "monaco-editor": "^0.21.2",
        "typescript": "^4.0.3"
    }
}
